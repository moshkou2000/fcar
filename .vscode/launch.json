  "version": "0.0.0",
  "configurations": [
    {
      "name": "Dev Debug iOS",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "program": "lib/main_dev.dart",
      "args": [
        "--flavor",
        "Debug-Dev",
        "--dart-define",
        "ABC_API_KEY=abcdefghigklmnopqrstuvwxyz",
        "--dart-define",
        "AUTH_USERNAME=admin",
        "--dart-define",
        "AUTH_PASSWORD=admin123"
      ]
    },
    {
      "name": "Dev Debug Android",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "program": "lib/main_dev.dart",
      "args": [
        "--flavor",
        "dev",
        "--dart-define",
        "ABC_API_KEY=abcdefghigklmnopqrstuvwxyz",
        "--dart-define",
        "AUTH_USERNAME=admin",
        "--dart-define",
        "AUTH_PASSWORD=admin123"
      ]
    }
  ]
}
